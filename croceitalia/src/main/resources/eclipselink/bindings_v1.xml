<jxb:bindings xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" version="2.1">
	<jxb:bindings schemaLocation="P730.xsd">

		<jxb:bindings node="//xs:element[@name='importo']">
			<jxb:property>
				<jxb:baseType>
					<jxb:javaType name="java.math.BigDecimal" parseMethod="eu.adapter.BigDecimalFormatter.parseDouble" printMethod="eu.adapter.BigDecimalFormatter.printDouble"/>
				</jxb:baseType>
			</jxb:property>
		</jxb:bindings>

		<jxb:bindings node="//xs:element[@name='dataPagamento']">
			<jxb:property>
				<jxb:baseType>
					<jxb:javaType name="javax.xml.datatype.XMLGregorianCalendar" parseMethod="eu.adapter.DateFormatter.parseDate" printMethod="eu.adapter.DateFormatter.printDate"/>
				</jxb:baseType>
			</jxb:property>
		</jxb:bindings>
		
		<jxb:bindings node="//xs:element[@name='dataEmissione']">
			<jxb:property>
				<jxb:baseType>
					<jxb:javaType name="javax.xml.datatype.XMLGregorianCalendar" parseMethod="eu.adapter.DateFormatter.parseDate" printMethod="eu.adapter.DateFormatter.printDate"/>
				</jxb:baseType>
			</jxb:property>
		</jxb:bindings>
		

		
		
	</jxb:bindings>
</jxb:bindings>
